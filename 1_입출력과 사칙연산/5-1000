using System;

namespace BaekJoon_C_
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] str = Console.ReadLine().Split(' ');

            int numA = int.Parse(str[0]);
            int numB = int.Parse(str[1]);

            Console.WriteLine(numA + numB);
        }
    }
}
