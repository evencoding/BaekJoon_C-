// 2021-08-15

using System;
using System.Collections.Generic;
using System.Text;

namespace BaekJoon_C_
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = Convert.ToInt32(Console.ReadLine());
            StringBuilder sb = new StringBuilder();

            for (int i = N; i > 0; i--){
                sb.Append(i + "\n");
            }
            Console.WriteLine(sb);
        }
    }
}